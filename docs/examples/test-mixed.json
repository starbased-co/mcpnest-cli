{
  "mcpServers": {
    "valid-npx": {
      "command": "npx",
      "args": ["@modelcontextprotocol/server-filesystem"]
    },
    "invalid-http": {
      "type": "http",
      "url": "https://api.example.com/mcp",
      "headers": {
        "Authorization": "Bearer token123"
      }
    },
    "invalid-path": {
      "type": "stdio",
      "command": "/usr/bin/python",
      "args": ["server.py"]
    },
    "valid-uvx": {
      "command": "uvx",
      "args": ["mcp-server-package"],
      "env": {
        "API_KEY": "test-key"
      }
    },
    "invalid-node": {
      "command": "node",
      "args": ["./server.js"]
    }
  }
}