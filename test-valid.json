{
  "mcpServers": {
    "github": {
      "type": "stdio",
      "command": "npx",
      "args": ["-y", "@modelcontextprotocol/server-github"]
    },
    "zen": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/starbased-co/zen-mcp-server.git",
        "zen-mcp-server"
      ],
      "env": {
        "GEMINI_API_KEY": "test-key"
      }
    }
  }
}